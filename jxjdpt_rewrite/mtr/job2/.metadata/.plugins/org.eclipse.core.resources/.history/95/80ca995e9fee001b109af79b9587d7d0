package com.sx.main;

import net.sf.json.JSONObject;

public class JsonMain {

	
	public static void main(String[] args) {
		
		JSONObject m = new JSONObject();
		m.put("1", "1");
		m.put("2", "2");
		m.put("3", "3");
		m.put("4", "4");
		m.put("5", "5");
		
		JSONObject m1 = new JSONObject();
		m1.put("6", m)
		System.out.println(m.toString());
		System.out.println(m1.toString());
		
	}
}
